<projectlist>
	<project>
		<name>Example Project</name>
		<scenario>
			<name>Example Scenario</name>
			<root name="Root" selectable3d="true" visible="true">
				<components/>
				<children>
					<node name="First Teapot" selectable3d="true" visible="true">
						<components>
							<renderGeometry>
								<meshSource>http://xml3d.github.io/xml3d-examples/examples/assets/res/assets.xml#teapot</meshSource>
							</renderGeometry>
						</components>
						<children/>
						<localTranslation x="-10.0" y="0.0" z="0.0"/>
						<localScale>1.0</localScale>
						<localRotation w="1.0" x="0.0" y="0.0" z="0.0"/>
					</node>
					<node name="Second Child" selectable3d="true" visible="true">
						<components/>
						<children>
							<node name="First Grandchild" selectable3d="true" visible="true">
								<components/>
								<children/>
								<localTranslation x="0.0" y="-10.0" z="0.0"/>
								<localScale>1.0</localScale>
								<localRotation w="1.0" x="0.0" y="0.0" z="0.0"/>
							</node>
							<node name="Second Grandchild" selectable3d="true" visible="true">
								<components/>
								<children/>
								<localTranslation x="0.0" y="10.0" z="0.0"/>
								<localScale>1.0</localScale>
								<localRotation w="1.0" x="0.0" y="0.0" z="0.0"/>
