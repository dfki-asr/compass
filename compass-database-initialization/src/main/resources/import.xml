				<localRotation w="1.0" x="0.0" y="0.0" z="0.0"/>
			</root>
		</scenario>
		<prefabSet>
			<name>Assets</name>
			<children/>
			<prefabs>
				<node name="Teapot" selectable3d="true" visible="true">
